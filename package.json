{
  "name": "playwright-study",
  "version": "1.0.0",
  "description": "Multipurpose Playwright study repository",
  "main": ".eslintrc.js",
  "directories": {
    "test": "test"
  },
  "devDependencies": {
    "chai": "^4.3.4",
    "dotenv": "^8.6.0",
    "eslint": "^7.30.0",
    "mocha": "^8.4.0",
    "playwright": "^1.13.0",
    "prettier": "^2.3.2"
  },
  "scripts": {
    "test": "mocha --timeout 10000 -r dotenv/config ./test/**/*.js",
    "test-match": "mocha --grep $MATCH --timeout 10000 -r dotenv/config ./test/**/*.js",
    "format-prettier": "prettier --print-width 120 --no-semi --single-quote --write **/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ltsuda/playwright-study.git"
  },
  "keywords": [],
  "author": "Leonardo Tsuda",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ltsuda/playwright-study/issues"
  },
  "homepage": "https://github.com/ltsuda/playwright-study#readme"
}
